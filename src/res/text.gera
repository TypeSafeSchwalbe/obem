
mod obem::res::Text

use obem::internal::res::Text::*
use std::opt

pub proc new(path) { return {
    path = path,
    data = #none unit,
    load = |self| |load_callback| {
        internal_load_text(self.path, |text| {
            self.data = #some text
            load_callback()
        })
    },
    loaded = |self| self.data |> opt::is_some(),
    get = |self| self.data |> opt::expect("Should be loaded!")
} }